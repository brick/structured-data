{
  "name": "brick/structured-data",
  "description": "Microdata, RDFa Lite & JSON-LD structured data reader",
  "type": "library",
  "keywords": [
    "Brick",
    "Structured data",
    "Microdata",
    "RDFa",
    "JSON-LD"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.2",
    "ext-dom": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "sabre/uri": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Brick\\StructuredData\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Brick\\StructuredData\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.1"
  }
}
